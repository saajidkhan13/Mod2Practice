<%= form_for @wrestler do |f| %>
<label>Wrestler Name:</label>
  <%= f.text_field :name %>
  <br>
  <br>
  <label>Weight:</label>
  <%= f.number_field :weight %>
  <br>
  <br>
  <label>Champion</label>
  <%= f.check_box :champion %>
  <br>
  <br>
  <label>Brand:</label>
  <%= f.collection_select :brand_id, Brand.all, :id, :name %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
